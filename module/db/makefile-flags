# compiler and flags
C=gcc
CFLAGS=-std=gnu11 -fPIC -I. -ggdb -Werror -DDEBUG -DDEBUGDB 
#CFLAGS=-std=gnu11 -fPIC -I. -Werror
LINKFLAGS=-Llib -ldb



